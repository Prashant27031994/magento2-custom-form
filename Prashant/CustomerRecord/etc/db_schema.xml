<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="prashant_customerrecord_record" resource="default" engine="innodb" comment="prashant_customerrecord_record Table">
		<column xsi:type="int" name="record_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="record_id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="varchar" comment="name" length="255"/>
		<column name="email" nullable="true" xsi:type="varchar" comment="email" length="255"/>
		<column name="country" nullable="true" xsi:type="varchar" comment="country" length="255"/>
	</table>
</schema>